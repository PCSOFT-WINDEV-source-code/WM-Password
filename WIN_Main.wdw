   -
     name : ChangePlane
     procedure_id : 1315896345190934373
     type_code : 14
     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      //ChangePlane (<nNewPlane> is int)
      //
      // Parameters:
      //	nNewPlane (integer): <specify the role of PLAN_DEFINITION>
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      procedure ChangePlane( nNewPlane is int )
      
      MyWindow.Plane = nNewPlane
      
      switch nNewPlane
      	case PLAN_HOME
      		
      	case PLAN_CHOICE_SECURE
      	case PLAN_DEFINE_DRAWING
      		CTPL_SecureDrawing.InitializeControl()
      		
      	case PLAN_CONFIRM_DRAWING
      		CTPL_SecureDrawing.InitializeControl()
      		
      	case PLAN_DEFINE_PASSWORD
      		EDT_CreationPassword1 = ""
      		SetFocus(EDT_CreationPassword1)
      		
      	case PLAN_CONFIRM_PASSWORD
      		EDT_CreationPassword2 = ""
      		SetFocus(EDT_CreationPassword2)
      		
      	case PLAN_ENTER_DRAWING
      		CTPL_SecureDrawing.InitializeControl()
      		
      	case PLAN_ENTER_PASSWORD
      		EDT_OpeningPassword = ""
      		SetFocus(EDT_OpeningPassword)
      		
      	case PLAN_UNLOCK
      		// Leave time in order for the user to see the animation
      		<if CibleExécution<>MacCatalyst>
      		Multitask(-100)
      		<end>
      		OpenMobileWindow(WIN_PasswordList)		
      end
      
     type : 458752
   -
     name : Initialization
     procedure_id : 1315896345190999909
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //Initialization ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      procedure Initialization()
      
      // Displays the input of the general password or its definition
      switch AuthenticationModeMain()
      	case MODE_AUTH_PATTERN
      		ChangePlane(PLAN_ENTER_DRAWING)
      	case MODE_AUTH_PASS
      		ChangePlane(PLAN_ENTER_PASSWORD)
      	other case
      		ChangePlane(PLAN_HOME)		
      end
     type : 458752
