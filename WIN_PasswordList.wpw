   -
     name : RefreshDisplay
     procedure_id : 1315896332300786824
     type_code : 14
     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      // RefreshDisplay ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      procedure RefreshDisplay()
      
      OPT_Search.Visible = True
      
      LooperDeleteAll(LOOP_Password)
      
      // Defines the query parameters according to the display mode
      switch gnCurrentDisplayMode
      	case MODE_PASSWORDS
      		QRY_PasswordList.pFavorite = Null
      		QRY_PasswordList.pCategoryID = (gnCategoryID=-1) ? Null else gnCategoryID
      		QRY_PasswordList.pSearch = (gsSearchFilter="") ? Null else gsSearchFilter
      		HExecuteQuery(QRY_PasswordList,hQueryDefault)
      		for each QRY_PasswordList
      			_AddElementIntoList()
      		end
      		
      	case MODE_FAVORITES
      		QRY_PasswordList.pFavorite = True
      		QRY_PasswordList.pCategoryID = Null
      		QRY_PasswordList.pSearch = (gsSearchFilter="") ? Null else gsSearchFilter
      		HExecuteQuery(QRY_PasswordList,hQueryDefault)
      		for each QRY_PasswordList
      			_AddElementIntoList()
      		end
      		
      	case MODE_CATEGORIES
      		// If passwords have no category
      		if HReadSeekFirst(Password,CategoryID,0) then
      			LooperAddLine(LOOP_Password,CATEGORY_NONE,"",0,IMG_CategoryList)
      		end
      		// Lists the categories with at least one password
      		HExecuteQuery(QRY_NonEmptyCategories)
      		for each QRY_NonEmptyCategories
      			LooperAddLine(LOOP_Password,QRY_NonEmptyCategories.CategoryName,Upper(QRY_NonEmptyCategories.CategoryName[[1]]),QRY_NonEmptyCategories.CategoryID,IMG_CategoryList)
      		end
      end
      
     type : 458752
   -
     name : ChangeViewMode
     procedure_id : 1315896332300852360
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //ChangeViewMode (<nMode> is int [, <nCategoryID> is 8-byte int])
      //
      // Parameters:
      //	nMode (integer): <specify the role of nMode>
      //	nCategoryID (8-byte integer - default value=-1): <specify the role of nCategoryID>
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      procedure ChangeViewMode( local nMode is int, local nCategoryID is 8-byte int = -1 )
      
      gnCurrentDisplayMode = nMode
      gnCategoryID = nCategoryID
      
      switch nMode
      	case MODE_PASSWORDS
      		switch gnCategoryID
      			case -1
      				MyWindow.Title = "Passwords"
      			case 0
      				MyWindow.Title = "No category"
      			other case
      				MyWindow.Title = CategoryName(nCategoryID)
      		end
      		OPT_View.Image = IMG_Category
      	case MODE_FAVORITES
      		MyWindow.Title = "Favorites"
      		OPT_View.Image = IMG_Password
      	case MODE_CATEGORIES
      		MyWindow.Title = "Categories"
      		OPT_View.Image = IMG_Favorites
      end
      
      RefreshDisplay()
     type : 458752
   -
     name : _AddElementIntoList
     procedure_id : 1315896332300917896
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      // _AddElementIntoList ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      procedure _AddElementIntoList()
      
      sCap is string
      sCap = gFontBold()+QRY_PasswordList.StcPassword
      if QRY_PasswordList.UserName<>"" then
      	if QRY_PasswordList.StcPassword<>"" then
      		sCap += gFontBold(False)+"  ("+QRY_PasswordList.UserName+")"		
      	else
      		sCap += QRY_PasswordList.UserName
      	end
      end
      
      nRow is int = LooperAddLine(LOOP_Password,sCap,Upper(sCap[[1]]),QRY_PasswordList.PasswordID,QRY_PasswordList.Picto)
      if QRY_PasswordList.Picto="" then
      	LOOP_Password[nRow].ATT_Image = IMG_DefaultPicto
      end
     type : 458752
