      procedure WIN_PasswordForm( local gnPasswordID is 8-byte int )
      
      constant
      	PLAN_VIEW = 1
      	PLAN_EDIT = 2
      	
      	// Lines of multiline zones that can be hidden according to data
      	VIEW_USER = 2
      	VIEW_PASSWORD = 3
      	VIEW_URL = 4
      	VIEW_NOTES = 5
      	VIEW_EXPIRATION = 7
      end
      
      gnCategoryID is 8-byte int
      gbModifiedForm is boolean = False
      gbCloseIgnoreModif is boolean = False
      
      
      if LoadParameter(PARAM_VIEW_PWD_EDIT)=1 then
      	DisplayPasswordInEdit()
      else
      	HidePasswordInEdit()
      end
      
      LoadAndDisplayPassword()
