     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      // ValidateInput ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      procedure bValidateInput()
      
      if EDT_StcPassword~="" then
      	ToastDisplay("Enter a caption for the password")
      	SetFocus(EDT_StcPassword)
      	return False
      end
      
      return True
