   -
     name : LoadAndDisplayPassword
     procedure_id : 1315896328003065282
     type_code : 14
     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      //LoadAndDisplayPassword ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      procedure LoadAndDisplayPassword()
      
      gnCategoryID = 0
      
      if gnPasswordID<>0 then
      	if HReadSeekFirst(Password,PasswordID,gnPasswordID) then
      		Password.AccessDate = SysDateTime()
      		HModify(Password)
      	else
      		gnPasswordID = 0
      	end
      end
      
      if gnPasswordID=0 then
      	HReset(Password)
      	Password.CreationDate = SysDateTime()
      	EDT_UserName = LoadParameter(PARAM_DEFAULT_USER)
      	EditMode()
      else
      	// Fill the viewing controls
      	MyWindow.Title = Password.StcPassword
      	STCView_User = Password.UserName
      	if Password.UserName~="" then
      		STC_User_name.State = Grayed
      		BTN_COPY2.State = Grayed
      	end
      	STCView_Password = DecryptBuffer(Password.Password)
      	STCView_URL = Password.URL
      	if Password.URL~="" then
      		STCView_URL = INFO_NOT_FILLED
      		BTN_Access.Visible = False
      	else
      		BTN_Access.Visible = True
      	end
      	STCView_Notes = Password.Description
      	STC_Notes.Visible = not (Password.Description~="")
      	if Password.ExpirationDate~="" then
      		STCView_Expiration = INFO_NOT_FILLED
      	else
      		if DateValid(Password.ExpirationDate) then
      			STCView_Expiration = DateToString(Password.ExpirationDate)
      		end
      	end
      	if HasExpired(Password.ExpirationDate) then
      		STCView_Expiration.BackgroundColor = PastelRed
      	end
      	CBOX_Favorite = Password.Favorite
      		
      	// Switch to view mode by default	
      	DispMode()
      
      	// Fill the edit controls
      	EDT_StcPassword = Password.StcPassword
      	EDT_UserName = Password.UserName
      	
      	EDT_Password = STCView_Password
      	EDT_PasswordVisible = STCView_Password
      	
      	if Password.Picto<>"" then
      		IMG_Picto = Password.Picto
      	end
      	EDT_URL = Password.URL
      	EDT_Expiration = Password.ExpirationDate
      	EDT_Description = Password.Description
      	gnCategoryID = Password.CategoryID
      end
      
      STC_AssociatedCategory = CategoryName(gnCategoryID)
      
      
     type : 458752
