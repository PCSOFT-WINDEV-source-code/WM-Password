   -
     name : SavePassword
     procedure_id : 1315896328003196354
     type_code : 14
     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      // SavePassword ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      procedure SavePassword()
      
      dtModif is datetime = SysDateTime()
      
      // Fill the edit controls
      Password.StcPassword = EDT_StcPassword
      Password.UserName = EDT_UserName
      if EDT_PasswordVisible.Visible = True then
      	Password.Password = digit(EDT_PasswordVisible)
      else
      	Password.Password = digit(EDT_Password)
      end
      Password.Picto = IMG_Picto
      Password.URL = EDT_URL
      Password.Favorite=CBOX_Favorite
      Password.ExpirationDate = EDT_Expiration
      Password.Description = EDT_Description
      Password.CategoryID = gnCategoryID
      if Password.NewRecord then
      	Password.CreationDate = dtModif
      end
      Password.ModificationDate = dtModif
      Password.AccessDate = dtModif
      
      HSave(Password)
      ToastDisplay("Changes saved")
